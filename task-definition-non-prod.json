{
    "family": "webapi-nonprod",
    "containerDefinitions": [
        {
            "image": "%ACCOUNT%.dkr.ecr.eu-central-1.amazonaws.com/%REPO%:%BUILD_TAG%",
            "name": "webapi-nonprod",
            "essential": true,
            "cpu": 10,
            "memoryReservation": 300,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                },
                {
                    "containerPort": 443,
                    "hostPort": 443,
                    "protocol": "tcp"
                },
                {
                    "containerPort": 5000,
                    "hostPort": 5000,
                    "protocol": "tcp"
                }
            ]
        }
    ]
}